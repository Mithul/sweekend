<%= simple_form_for(@registration) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.input :regtype %>
    <%= f.input :organization %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
